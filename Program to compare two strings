#include <stdio.h>
#include <ctype.h>
#include <string.h>
int main()
{
    char str[1000];
    printf("Enter first string = ");
    fgets(str, 1000, stdin);
    char str2[1000];
    printf("Enter second string = ");
    fgets(str2, 1000, stdin);
    int value = strcmp(str, str2);
    if (value == 0)
    {
        printf("Strings are same");
    }
    else
    {
        printf("Strings are not same");
    }
    return 0;
}
